{
  "rewrites": [
    {
      "source": "/qr",
      "destination": "/qr/index.html"
    },
    {
      "source": "/qr/",
      "destination": "/qr/index.html"
    },
    {
      "source": "/qr/wifi",
      "destination": "/qr/wifi/index.html"
    },
    {
      "source": "/qr/wifi/",
      "destination": "/qr/wifi/index.html"
    },
    {
      "source": "/shortener",
      "destination": "/shortener/index.html"
    },
    {
      "source": "/shortener/",
      "destination": "/shortener/index.html"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/([a-zA-Z0-9]{7})",
      "destination": "/api/$1"
    }
  ]
}
